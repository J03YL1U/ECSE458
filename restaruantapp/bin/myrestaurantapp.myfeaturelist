concept Seat
	int number

concept Table
	int number
	Seat seat 1..*
	String fakeatt 1..0
	
constraints
	Table.number > 0
	Seat.number > 0
	
keys
	Table.number unique
	Seat.number unique
	
properties
	Table.fakeatt lazy
	
features
	Add Table
	Remove Table
	Update Seat.number
	Display Table
	Add Table.seat
	Remove Table.seat