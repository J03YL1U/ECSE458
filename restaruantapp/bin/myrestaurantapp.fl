concept RestoApp

concept Seat
	int number
	
concept Table
	int number
	String bogusAttribute
	Seat seats 1..*
	
constraints
	Table.number > 0 "The table number must be greater than 0"
	Seat.number > 0 "The seat number must be greater than 0"
	
keys
	Table.number unique
	Table.seats index
	
properties
	RestoApp root
	
features
	Add Table
	Remove Table
	Update Seat.number
	Display Table
	Add Table.seats
	Remove Table.seats